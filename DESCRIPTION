Package: nwmHistoric
Title: National Water Model Historic Data
Version: 0.0.0.9000
Authors@R: 
    person(given = "Mike", family = "Johnson", role = c("aut", "cre"), email = "jmj00@ucsb.edu", comment = c(ORCID = "0000-0002-5288-8350"))
